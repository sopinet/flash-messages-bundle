{% for type, flashMessages, position in app.session.flashbag.all() %}
	{% for flashMessage in flashMessages %}
	<script>
		$.msgGrowl({
			'type': '{{ type }}',
			'title': '{{ flashMessage }}',
			'text': '{{ flashMessage }}',
			'position': 'bottom-center',
			'sticky':false
		});
	</script>
	{% endfor %}
{% endfor %}
